<?php

/**
 * Implements hook_install().
 * 
 * Uninstalls some modules.
 */
function loopduplicateutils_install() {
  $uninstall_module_list = array(
    'overlay',
    'color',
    'comment',
    'dashboard',
    'dblog',
    'help',
    'toolbar',
  );
  $utils = new LoopduplicateutilsModule();
  $utils->uninstall($uninstall_module_list);
  cache_clear_all();
}
